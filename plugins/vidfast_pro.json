{
  "id": "vidfast_pro",
  "name": "VIDFAST.PRO",
  "version": "1.0",
  "author": "Community",
  "type": "community",

  "description": "Fast video streaming source with auto play, auto next and multi-language subtitles (if available).",

  "supports": ["movie", "tv"],
  "playback": "embed",

  "base_url": "https://vidfast.pro",

  "movie_path": "/movie/{tmdbId}?autoPlay=true",
  "tv_path": "/tv/{tmdbId}/{season}/{episode}?autoPlay=true&nextButton=true&autoNext=true",

  "headers": {
    "Referer": "https://vidfast.pro/",
    "Origin": "https://vidfast.pro"
  },

  "enabled_by_default": false,
  "disclaimer_required": true,

  "image_url": "https://vidfast.pro/favicon.ico",
  "icon_color": "#16A085",
  "language": "en"
}
